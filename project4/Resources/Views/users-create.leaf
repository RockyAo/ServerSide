#set("title"){ Create account }

#set("body") {
    <h1>Create Account</h1>
    <form method="post">
        <p>Username: <input type="text" name="user-name"/></p>
        <p>Password: <input type="password" name="password"/></p>
        <p><input type="submit" class="btn btn-lg btn-primary"/></p>
    </form>
}

#embed("master")